@model IEnumerable<Developer>

@{
    ViewBag.Title = "All Developers";
}

<div class="mtop20">
    <a asp-controller="Home" asp-action="AddDeveloper" class="btn btn-success">Add New Developer</a>
</div>


<div class="mtop20">
    <table id="tblPropertyinfo" class="table table-bordered table-hover table-striped ">
        <thead class="thead-light">
            <tr>
                <th id="Id">ID</th>
                <th id="name" class="hideCol hideHideCol_2">Name</th>
                <th id="age" class="hideCol hideHideCol_3">Age</th>
                <th id="jobtitle" class="hideCol hideHideCol_3">Job Title</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                @if (Model.Count() > 0)
                {
                    @foreach (var developer in Model)

                    {
                        <tr>
                            <td>@developer.developerId</td>
                            <td>@developer.name</td>
                            <td>@developer.age</td>
                            <td>@developer.jobTitle</td>           
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>